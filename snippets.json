{
	"s1": {
		"prefix": "s1",
		"body": [
			":root {",
			"  --preto: #121212;",
			"  --branco: #E9F0EC;",
			"  --cinza: #303030;",
			"  --destaque: #232638;",
			"}",
			"body {",
			"  font-family: 'Open Sans', sans-serif;",
			"  background-color: var(--branco);",
			"  color: var(--preto);",
			"  line-height: 1.3;",
			"  margin: 0;",
			"}",
			"button {",
			"  border: none;",
			"  font-size: 1rem;",
			"  cursor: pointer;",
			"}",
			"h1, h2, p {",
			"  margin: 0;",
			"  font-weight: normal;",
			"}",
			"img {",
			"  display: block;",
			"  max-width: 100%;",
			"}"
		],
		"description": "s1"
	},
	"s2": {
		"prefix": "s2",
		"body": [
			"<script>",
			"  import '$$lib/estilo.css';",
			"</script>"
		],
		"description": "s2"
	},
	"s3": {
		"prefix": "s3",
		"body": [
			"<!-- Google Fonts -->",
			"<link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">",
			"<link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>",
			"<link href=\"https://fonts.googleapis.com/css2?family=Open+Sans&display=swap\" rel=\"stylesheet\">"
		],
		"description": "s3"
	},
	"s4": {
		"prefix": "s4",
		"body": [
			"import img from '$$lib/img.png';"
		],
		"description": "s4"
	},
	"s5": {
		"prefix": "s5",
		"body": [
			"<!-- HTML -->",
			"<main>",
			"  <div class=\"imagem\">",
			"    <div class=\"bg\"></div>",
			"    <img src=\"{img}\" alt=\"Desenvolvedor Full Stack\">",
			"  </div>",
			" ",
			"  <div class=\"conteudo\">",
			"    <h1>Desenvolvedor Full Stack</h1>",
			"    <h2>Precisa de um site ou app para o seu projeto?</h2>",
			"    <button class=\"btn\">Entre em contato</button>",
			"  </div>",
			"</main>"
		],
		"description": "s5"
	},
	"s6": {
		"prefix": "s6",
		"body": [
			"<!-- CSS -->",
			"<style>",
			"  main {",
			"    display: grid;",
			"    grid-template-columns: 230px 1fr;",
			"    gap: 20px;",
			"    align-items: start;",
			"    padding: 40px;",
			"  }",
			"</style>"
		],
		"description": "s6"
	},
	"s7": {
		"prefix": "s7",
		"body": [
			".bg {",
			"  background-color: var(--destaque);",
			"  width: 220px;",
			"  height: 335px;",
			"}"
		],
		"description": "s7"
	},
	"s8": {
		"prefix": "s8",
		"body": [
			".imagem {",
			"  position: relative;",
			"}"
		],
		"description": "s8"
	},
	"s9": {
		"prefix": "s9",
		"body": [
			"img {",
			"  position: relative;",
			"}"
		],
		"description": "s9"
	},
	"s10": {
		"prefix": "s10",
		"body": [
			".conteudo {",
			"  margin-top: 8px;",
			"  display: grid;",
			"  gap: 8px;",
			"}"
		],
		"description": "s10"
	},
	"s11": {
		"prefix": "s11",
		"body": [
			".btn {",
			"  display: inline-block;",
			"  /* width: fit-content; */",
			"  background-color: var(--destaque);",
			"  text-decoration: none;",
			"  color: var(--branco);",
			"  padding: 12px 16px;",
			"  margin-bottom: 16px;",
			"}"
		],
		"description": "s11"
	},
	"s12": {
		"prefix": "s12",
		"body": [
			"on:click={notificar}"
		],
		"description": "s12"
	},
	"s13": {
		"prefix": "s13",
		"body": [
			"<footer>",
			"  <span>© Desenvolvedor Full Stack, 2023</span>",
			"</footer>"
		],
		"description": "s13"
	},
	"s14": {
		"prefix": "s14",
		"body": [
			"footer {",
			"  background-color: #fff;",
			"  height: 60px;",
			"  display: flex;",
			"  align-items: center;",
			"  padding: 0 40px;",
			"  font-size: calc(14 / 16 * 1rem);",
			"  position: absolute;",
			"  width: calc(100% - 80px);",
			"  bottom: 0;",
			"}"
		],
		"description": "s14"
	},
	"s15": {
		"prefix": "s15",
		"body": [
			"<div class=\"preferencia\">",
			"  <p>Você prefere via e-mail ou WhatsApp?</p>",
			"  <div class=\"btns\">",
			"    <button class=\"btn\">E-mail</button>",
			"    <button class=\"btn\">WhatsApp</button>",
			"  </div>",
			"</div>",
			"",
			"<div class=\"resultado-email\">",
			"  <p>Combinado, aguardo seu contato via e-mail!</p>",
			"  <a href=\"/\" class=\"btn\">teste@teste.com.br</a>",
			"</div>",
			"",
			"<div class=\"resultado-whatsapp\">",
			"  <p>Combinado, aguardo seu contato via WhatsApp!</p>",
			"  <a href=\"/\" class=\"btn\">(99) 99999-9999</a>",
			"</div>"
		],
		"description": "s15"
	},
	"s16": {
		"prefix": "s16",
		"body": [
			"p {",
			"  margin-bottom: 8px;",
			"}"
		],
		"description": "s16"
	},
	"s17": {
		"prefix": "s17",
		"body": [
			"{#if preferencia === 'escolher'}",
			"{/if}",
			"{#if preferencia === 'email'}",
			"{/if}",
			"{#if preferencia === 'whatsapp'}",
			"{/if}"
		],
		"description": "s17"
	},
	"s18": {
		"prefix": "s18",
		"body": [
			"on:click={() => preferencia = 'escolher'}"
		],
		"description": "s18"
	},
	"s19": {
		"prefix": "s19",
		"body": [
			"$: console.log(preferencia);"
		],
		"description": "s19"
	},
	"s20": {
		"prefix": "s20",
		"body": [
			"on:click={() => preferencia = 'email'}"
		],
		"description": "s20"
	},
	"s21": {
		"prefix": "s21",
		"body": [
			"on:click={() => preferencia = 'whatsapp'}"
		],
		"description": "s21"
	},
	"s22": {
		"prefix": "s22",
		"body": [
			"@keyframes mostrar {",
			"  0% {",
			"    opacity: 0;",
			"    transform: translateX(-20px);",
			"  }",
			"  100% {",
			"    opacity: 1;",
			"    transform: translateX(0);",
			"  }",
			"}"
		],
		"description": "s22"
	},
	"s23": {
		"prefix": "s23",
		"body": [
			"opacity: 0;",
			"animation: mostrar 500ms forwards;",
			"animation-delay: 500ms;"
		],
		"description": "s23"
	},
	"s24": {
		"prefix": "s24",
		"body": [
			".preferencia,",
			".resultado-email,",
			".resultado-whatsapp {",
			"  animation: mostrar 500ms;",
			"}"
		],
		"description": "s24"
	},
	"s25": {
		"prefix": "s25",
		"body": [
			"@media(max-width: 700px) {",
			"  main {",
			"    grid-template-columns: 1fr;",
			"  }",
			"  .conteudo {",
			"    grid-row: 1;",
			"  }",
			"  .imagem {",
			"    max-width: 230px;",
			"    margin-left: -10px;",
			"  }",
			"}"
		],
		"description": "s25"
	}
}